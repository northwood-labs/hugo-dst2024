{
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "svgo": "2.8.0"
      }
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "snyk"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.10.0"
  },
  "browserslist": [
    ">0.3%, defaults, fully supports es6-module, maintained node versions"
  ],
  "source": [
    "layouts/index.html",
    "layouts/_default/*.html",
    "layouts/partials/.*/*.html",
    "assets/css/_tailwind.css",
    "assets/js/_core.js"
  ],
  "targets": {
    "default": {
      "distDir": "../../dist",
      "publicUrl": "/static",
      "context": "browser",
      "outputFormat": "global"
    }
  },
  "scripts": {
    "start": "TAILWIND_MODE=watch NODE_ENV=development npm-run-all pre:twcss --parallel dev:* cp",
    "lint": "npm-run-all eslint markdownlint",
    "eslint": "eslint --fix assets/js/_*.js",
    "markdownlint": "markdownlint --fix '**/*.md' --ignore 'node_modules'",
    "build": "NODE_ENV=production npm-run-all pre:twcss --parallel prod:* cp",
    "cp": "cp -vf ../../dist/assets/js/_core.js assets/js/index.js && cp -vf ../../dist/assets/js/_core.js.map assets/js/_core.js.map",
    "pre:twcss": "tailwindcss --input ./assets/css/_tailwind.css --output ./assets/css/index.css",
    "dev:twcssw": "tailwindcss --input ./assets/css/_tailwind.css --output ./assets/css/index.css --watch",
    "dev:parcel": "parcel watch --detailed-report=10",
    "prod:twcss": "tailwindcss --input ./assets/css/_tailwind.css --output ./assets/css/index.css --minify",
    "prod:parcel": "rm -f assets/js/index.js && parcel build --detailed-report=10 --no-cache --log-level=verbose",
    "test": "jest"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "preact": "^10.26.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@parcel/bundler-default": "^2.14.1",
    "@parcel/bundler-library": "^2.14.1",
    "@parcel/compressor-brotli": "^2.14.1",
    "@parcel/compressor-gzip": "^2.14.1",
    "@parcel/config-default": "^2.13.3",
    "@parcel/namer-default": "^2.14.1",
    "@parcel/optimizer-css": "^2.14.1",
    "@parcel/optimizer-esbuild": "^2.14.1",
    "@parcel/optimizer-htmlnano": "^2.14.1",
    "@parcel/packager-css": "^2.14.1",
    "@parcel/packager-html": "^2.14.1",
    "@parcel/packager-js": "^2.14.1",
    "@parcel/packager-raw": "^2.14.1",
    "@parcel/reporter-cli": "^2.14.1",
    "@parcel/resolver-default": "^2.14.1",
    "@parcel/resolver-glob": "^2.14.1",
    "@parcel/transformer-css": "^2.14.1",
    "@parcel/transformer-html": "^2.14.1",
    "@parcel/transformer-js": "^2.13.3",
    "@parcel/transformer-postcss": "^2.14.1",
    "@parcel/transformer-raw": "^2.14.1",
    "@parcel/transformer-vue": "^2.14.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@tailwindcss/cli": "^4.0.14",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.14",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/preact": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.14.1",
    "parcel-reporter-clean-dist": "^1.0.4",
    "parcel-resolver-ignore": "^2.2.0",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-custom-properties": "^14.0.4",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "posthtml-doctype": "^1.1.1",
    "prettier": "^3.5.3",
    "prettier-plugin-go-template": "^0.0.15",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "process": "^0.11.10",
    "snyk": "^1.1296.0",
    "svgo": "^3.3.2",
    "tailwindcss": "^4.0.14"
  },
  "parcelIgnore": [
    "{{.*}}",
    "/images/.*"
  ]
}
